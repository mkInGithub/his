<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.model.mapper.MedicalRecordMapper"  >
	
	<insert id="saveMedicalRecord"  parameterType="MedicalRecord" >
		insert into medicalrecord(pid,description,medicalhistory,familyhistory,initialresult,result,finalresult,status,operator,operatedate)
		values(#{pid},#{description},#{medicalhistory},#{familyhistory},#{initialresult},#{result},#{finalresult},#{status},#{operator},#{operatedate})
	</insert>
	
</mapper>










